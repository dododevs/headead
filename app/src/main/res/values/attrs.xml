<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textStyle" format="enum">
        <enum name="black" value="0"/>
        <enum name="bold" value="1"/>
        <enum name="extraBold" value="2"/>
        <enum name="light" value="3"/>
        <enum name="medium" value="4"/>
        <enum name="regular" value="5"/>
        <enum name="semiBold" value="6"/>
        <enum name="thin" value="7"/>
        <enum name="italic" value="8"/>
    </attr>
    <declare-styleable name="MontserratTextView">
        <attr name="textStyle"/>
    </declare-styleable>
    <declare-styleable name="LektonTextView">
        <attr name="textStyle"/>
    </declare-styleable>
    <declare-styleable name="ExpandedCoordinatorLayout">
        <attr name="expandedOffset" format="dimension"/>
    </declare-styleable>
</resources>